{
  "name": "street-cred-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn prepare && vite",
    "build": "yarn prepare && cross-env NODE_ENV=production tsc && vite build",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src",
    "postinstall": "yarn prepare",
    "prepare": "yarn generate-css-types",
    "generate-css-types": "tailwindcss-classnames -o src/classnames/tailwind.ts && prettier --write src/classnames/tailwind.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@metamask/eth-sig-util": "^4.0.0",
    "@metamask/providers": "^8.1.1",
    "@vitejs/plugin-react": "^1.1.4",
    "axios": "^0.24.0",
    "buffer": "^6.0.3",
    "copy-to-clipboard": "^3.3.1",
    "ethers": "^5.5.3",
    "preact": "^10.6.4",
    "react-router-dom": "^6.2.1",
    "valtio": "^1.2.7"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.1.7",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-no-relative-import-paths": "^1.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-valtio": "^0.4.1",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "react-scripts": "^5.0.0",
    "rollup-plugin-visualizer": "^5.5.2",
    "tailwindcss": "^3.0.12",
    "tailwindcss-classnames": "^3.0.1",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "vite-tsconfig-paths": "^3.3.17"
  },
  "packageManager": "yarn@3.1.1"
}
