{
  "name": "seal-cred-frontend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn generate-css-types && vite",
    "build": "yarn generate-css-types && cross-env NODE_ENV=production tsc && vite build",
    "preview": "yarn build && yarn vite preview",
    "pretty": "prettier --check src",
    "lint": "yarn pretty && eslint --max-warnings 0 --ext ts,tsx,json src",
    "postinstall": "yarn generate-css-types",
    "generate-css-types": "tailwindcss-classnames -o src/classnames/tailwind.ts && prettier --write src/classnames/tailwind.ts"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@big-whale-labs/constants": "^0.0.18",
    "@big-whale-labs/poseidon": "^0.0.5",
    "@big-whale-labs/seal-cred-ledger-contract": "0.0.6",
    "@coinbase/wallet-sdk": "^3.3.0",
    "@walletconnect/web3-provider": "^1.7.8",
    "axios": "^0.27.2",
    "envalid": "^7.3.1",
    "eth-rpc-errors": "^4.0.3",
    "ethers": "^5.6.8",
    "ffjavascript": "^0.2.55",
    "preact": "^10.8.1",
    "qr-code-styling": "^1.6.0-rc.1",
    "react-resize-detector": "^7.1.2",
    "react-router-dom": "^6.3.0",
    "react-toastify": "^9.0.4",
    "secure-ls": "^1.2.6",
    "tailwind-scrollbar-hide": "^1.1.7",
    "valtio": "^1.6.1",
    "web3modal": "^1.9.8"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@preact/preset-vite": "^2.2.0",
    "@rollup/plugin-inject": "^4.0.4",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vitejs/plugin-react": "^1.3.2",
    "assert-browserify": "^2.0.0",
    "autoprefixer": "^10.4.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-no-relative-import-paths": "^1.3.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-sort-imports-es6-autofix": "^0.6.0",
    "eslint-plugin-valtio": "^0.4.4",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "react-scripts": "^5.0.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-visualizer": "^5.6.0",
    "tailwindcss": "^3.1.3",
    "tailwindcss-classnames": "^3.0.5",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vite-plugin-remove-console": "^1.0.0",
    "vite-tsconfig-paths": "^3.5.0"
  },
  "packageManager": "yarn@3.2.1"
}
